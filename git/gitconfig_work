[alias]
    bd=branch -D
    bs=!git b master
    commit-with-issue-tag="!f () {                                                                   \
            git commit -m \"$(git symbolic-ref --short HEAD | cut -d- -f1 | sed 's/[^0-9]*//g') $@\"; \
        }; f"
    ms=merge master
    ps=pull origin master
