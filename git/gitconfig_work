[alias]
    bd=branch -D
    bs=!git b development
    commit-with-issue-tag="!f () {                                                                                  \
            git commit -m \"$(git symbolic-ref --short HEAD | cut -d/ -f3 | egrep -o '\\w+-[0-9]+' | head -1) $@\"; \
        }; f"
    ms=merge development
    ps=pull origin development
