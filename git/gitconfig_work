[alias]
    bd=branch -D
    bs=!git b master
    commit-with-issue-tag="!f () {                                                   \
            git commit -m \"$(git symbolic-ref --short HEAD | sed 's/--.*//g') $@\"; \
        }; f"
    ms=merge master
    ps=pull origin master
