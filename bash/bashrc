alias ll='ls -l'
alias gti=git
alias vom=vim
alias ne='node -e'

alias yarni='rm -rf node_modules yarn.lock package-lock.json; yarn'
alias npmi='rm -rf node_modules package-lock.json; npm install'

alias rnra='react-native run-android'
